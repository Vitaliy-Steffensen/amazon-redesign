{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "react-app",
    "prettier",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ]
}
